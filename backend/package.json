{
  "name": "ken-apis",
  "private": false,
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start-auth": "nest start auth",
    "start-users": "nest start users",
    "dev-auth": "nest start auth --watch",
    "dev-users": "nest start users --watch",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "prisma": "npx prisma studio",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/jest/bin/jest --runInBand"
  },
  "dependencies": {
    "@bull-board/express": "^6.12.0",
    "@nestjs/bull": "^11.0.3",
    "@nestjs/common": "11",
    "@nestjs/config": "4",
    "@nestjs/core": "11",
    "@nestjs/jwt": "11",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/passport": "11",
    "@nestjs/platform-express": "11",
    "@nestjs/schedule": "^6.0.0",
    "@nestjs/swagger": "8",
    "@prisma/client": "^5.13.0",
    "@saemhco/nestjs-html-pdf": "^0.1.0",
    "@types/cron": "^2.4.3",
    "@types/multer": "^1.4.11",
    "@types/passport-jwt": "^3.0.13",
    "axios": "^1.9.0",
    "bcrypt": "^5.1.1",
    "bull": "^4.16.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "date-fns": "^3.6.0",
    "dotenv": "^16.4.5",
    "ethers": "^6.15.0",
    "exceljs": "^4.4.0",
    "google-auth-library": "^10.3.0",
    "ioredis": "^5.7.0",
    "mexc-api-sdk": "^1.0.3",
    "multer": "^1.4.5-lts.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "prisma": "^5.13.0",
    "reflect-metadata": "^0.1.13",
    "resend": "^3.2.0",
    "rxjs": "^7.8.1",
    "smsapi": "^2.0.15",
    "typescript-nestjs-swagger-plugin": "^2.0.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.2",
    "@nestjs/schematics": "^10.1.1",
    "@nestjs/testing": "^10.3.9",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.64",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^24.2.1",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/"
    ]
  }
}